/*Program to calculate factorial using function-09/09/2015*/
/*With parameter and no return type*/
#include<stdio.h>
#include<conio.h>
void fact(int);	//calling function

void main()
{
	int num;
	printf("Enter the number:");
	scanf("%d",&num);
	clrscr();
	fact(num);	//function call
getch();
}
void fact(int temp)	//called function
{
	int iter=1,fact=1;
	do
	{
		fact=fact*iter;
		iter++;
	}while(iter<=temp);
	printf("\nFactorial of %d is %d.",temp,fact);
}


